{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Wolfgang.Extensions.IAsyncEnumerable/Wolfgang.Extensions.IAsyncEnumerable.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/**/*.{md,yml}",
          "api/**/toc.yml",
          "api/**/index.md",
          "api/**/*.yml",
          "index.md",
          "toc.yml"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "modern-dark"
    ],
    "globalMetadata": {
      "_appName": "IAsyncEnumerable Extensions",
      "_appTitle": "Wolfgang.Extensions.IAsyncEnumerable Documentation",
      "_enableSearch": true,
      "_appFooter": "Made with DocFX",
      "_disableSidebar": false,
      "_disableTocFilter": false,
      "_enableDarkMode": true,
      "pdf": true
    }
  }
}
